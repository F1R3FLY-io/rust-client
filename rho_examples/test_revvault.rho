// Test RevVault lookup
new
  lookup(`rho:registry:lookup`),
  stdout(`rho:io:stdout`),
  retCh
in {
  stdout!("Looking up RevVault...") |
  lookup!(`rho:rchain:revVault`, *retCh) |
  for (@result <- retCh) {
    match result {
      Nil => stdout!("ERROR: RevVault not found in registry!")
      _ => stdout!(["RevVault found:", result])
    } |
    retCh!(result)
  }
}
